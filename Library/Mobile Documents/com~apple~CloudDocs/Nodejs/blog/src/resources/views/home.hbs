<body>
    <div  class="image">
        <img id="img" onclick="changeimg()" src="img/img5.png" alt="">
    </div>
    <script>
        var index = 1;
        function changeimg(){
             var images = ["img/img5.png", "img/img6.png", "img/img7.png", "img/img8.png"];
             document.getElementById('img').src = images[index];
              index++;
            if(index == 4){
                index = 0;
            }      
        } 
        setInterval(changeimg, 2900);  
    </script>
</body>







